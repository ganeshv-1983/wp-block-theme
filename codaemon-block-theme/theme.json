{
	"$schema": "https://schemas.wp.org/wp/6.4/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [],
			"gradients": [],
			"palette": [
				{
					"color": "#000000",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#AD4614",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#FAFAFA",
					"name": "Light Gray",
					"slug": "light-gray"
				},
				{
					"color": "#FFFFFF",
					"name": "White",
					"slug": "white"
				}
			]
		},
		"custom": {
			"h-spacing": "2.375rem",
			"lineHeight": {
				"body": 1.5,
				"customlineheight": 1,
				"heading": 1.1999999999999999555910790149937383830547332763671875
			},
			"spacing": {
				"outer": "30px"
			},
			"v-spacing": "1.25rem"
		},
		"layout": {
			"contentSize": "1000px",
			"wideSize": "1600px"
		},
		"spacing": {
			"units": [
				"px",
				"%",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"customFontSize": false,
			"dropCap": false,
			"fluid": false,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "ABCSocial-Medium",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "500",
							"provider": "local",
							"src": "file:./assets/fonts/ABCSocial-Medium.woff2"
						}
					],
					"fontFamily": "\"ABCSocial-Medium\", sans-serif",
					"name": "ABCSocial-Medium",
					"slug": "ABCSocial_Medium"
				},
				{
					"fontFace": [
						{
							"fontFamily": "abc_social_condensed_unliceRg",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"provider": "local",
							"src": "file:./assets/fonts/abcsocialcondensed-regular-trial-webfont.woff2"
						}
					],
					"fontFamily": "\"abc_social_condensed_unliceRg\", sans-serif",
					"name": "abc_social_condensed_unliceRg",
					"slug": "abc_social_condensed_unliceRg"
				},
				{
					"fontFace": [
						{
							"fontFamily": "ABCSocial-Light",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "300",
							"provider": "local",
							"src": "file:./assets/fonts/ABCSocial-Light.woff2"
						}
					],
					"fontFamily": "\"ABCSocial-Light\", sans-serif",
					"name": "ABCSocial-Light",
					"slug": "ABCSocial_Light"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"max": "1.2rem",
						"min": "1rem"
					},
					"name": "Small",
					"size": "16px",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "1rem",
						"min": "1.2rem"
					},
					"name": "Medium",
					"size": "24px",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "1rem",
						"min": "1rem"
					},
					"name": "Large",
					"size": "30px",
					"slug": "large"
				}
			],
			"textDecoration": false
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/button": {
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--secondary)"
				},
				"spacing": {
					"padding": {
						"bottom": "10px",
						"left": "25px",
						"right": "25px",
						"top": "10px"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--regular)"
				}
			},
			"core/columns": {
				"spacing": {
					"margin": {
						"bottom": "0px"
					}
				}
			},
			"core/cover": {
				"spacing": {
					"padding": {
						"bottom": "30px",
						"left": "30px",
						"right": "30px",
						"top": "30px"
					}
				}
			},
			"core/group": {
				"spacing": {
					"padding": "0px"
				}
			},
			"core/image": {
				"spacing": {
					"margin": {
						"bottom": "0px"
					}
				}
			},
			"core/navigation": {
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--40)"
				}
			},
			"core/paragraph": {
				"spacing": {
					"margin": {
						"bottom": "20px",
						"top": "0px"
					}
				}
			},
			"core/spacer": {
				"spacing": {
					"margin": {
						"top": "0 !important"
					}
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--Secondary)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--ABCSocial_Medium)",
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "var(--wp--custom--font-weight--regular)",
					"lineHeight": "var(--wp--custom--line-height--heading)",
					"textTransform": "uppercase"
				}
			},
			"h2": {
				"spacing": {
					"margin": {
						"bottom": "40px",
						"left": "0px",
						"right": "0px",
						"top": "20px"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--ABCSocial_Medium)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "var(--wp--custom--font-weight--regular)",
					"lineHeight": "var(--wp--custom--line-height--heading)",
					"textTransform": "uppercase"
				}
			},
			"h3": {
				"spacing": {
					"margin": {
						"bottom": "30px",
						"left": "0px",
						"right": "0px",
						"top": "10px"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"textTransform": "uppercase"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "300"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--secondary)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"textDecoration": "none"
				}
			}
		},
		"spacing": {
			"blockGap": "0",
			"margin": {
				"bottom": "20px",
				"left": "0px",
				"right": "0px",
				"top": "0px"
			},
			"padding": {
				"bottom": "0px",
				"left": "0px",
				"right": "0px",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--ABCSocial_Light)",
			"fontSize": "var(--wp--preset--font-size--regular)",
			"fontWeight": "var(--wp--custom--font-weight--regular)",
			"lineHeight": "var(--wp--custom--line-height--customlineheight)",
			"textDecoration": "none"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"version": 2
}